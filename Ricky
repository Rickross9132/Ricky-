  const baseUrl = document.getElementById('g_id_onload').dataset.baseUrl
  const url = '/am/json/realms/root/realms/authtree/authenticate'
  const params = '?authIndexType=service&authIndexValue=googleSignIn'
  const errorUrl = '/am/XUI/#failedLogin'
  var request = new XMLHttpRequest();

  request.onload = function(){
    if(request.status >=200 && request.status <400){
      var responseObj = request.response;
      if(responseObj && responseObj.successUrl) {
        window.location.href = responseObj.successUrl;
      } else {
        window.location.href = errorUrl;
      }
    } else if (request.status === 403){
      window.location.href = baseUrl;
    }
  }

  request.onerror = function(){
    window.location.href = errorUrl;
  }

  request.responseType = 'json';
  request.withCredentials = true;
  request.open('POST', baseUrl + url + params, true);
  request.setRequestHeader('Content-Type', 'application/json');
  request.setRequestHeader('Authorization', googleUser.credential);
  request.send();
}</script><div class="footer-nav"><div class="container"><div class="columns"><div class="column is-full"><a class="nav-image-link" href="/" aria-label="Home"><div class="image"><picture><source sizes="(max-width: 380px) 380px, (max-width: 460px) 460px" srcset="/images/logo-saucelabs-inverted.png?fm=webp&w=380 380w, 
